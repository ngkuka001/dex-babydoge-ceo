{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:test": "vite --mode test",
    "dev:staging": "vite --mode staging",
    "build": "tsc && vite build",
    "build:staging": "tsc && vite build --mode staging",
    "build:test": "tsc && vite build --mode test",
    "preview": "vite preview",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --color",
    "format": "prettier --write .",
    "prepare": "husky install",
    "generate-abi-type": "typechain --target ethers-v5 --out-dir src/abi/types './src/abi/**/*.abi.json' --always-generate-overloads",
    "postinstall": "yarn run generate-abi-type"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn format",
      "git add ."
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@ethersproject/constants": "^5.5.0",
    "@ethersproject/contracts": "^5.4.1",
    "@ethersproject/providers": "^5.4.2",
    "@ethersproject/units": "^5.4.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@rollup/plugin-inject": "^4.0.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@walletconnect/browser": "^1.0.0",
    "@walletconnect/qrcode-modal": "^1.4.1",
    "@web3-react/abstract-connector": "^6.0.7",
    "@web3-react/core": "^6.1.9",
    "@web3-react/frame-connector": "^6.0.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "6.2.4",
    "antd": "^4.23.6",
    "axios": "^0.26.0",
    "browserify-zlib": "^0.2.0",
    "classnames": "^2.3.1",
    "ethers": "^5.5.4",
    "formik": "^2.2.9",
    "gsap": "^3.9.1",
    "i18next": "^21.6.13",
    "i18next-browser-languagedetector": "^6.1.3",
    "lodash": "^4.17.21",
    "node-sass": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.5",
    "react-infinite-scroller": "^1.2.5",
    "react-lazyload": "^3.2.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "sass": "^1.49.9",
    "socket.io-client": "^4.4.1",
    "stream-browserify": "^3.0.0",
    "vite-plugin-remove-console": "^0.0.7",
    "web-vitals": "^2.1.4",
    "web3": "^1.7.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@rollup/plugin-inject": "^4.0.4",
    "@typechain/ethers-v5": "^9.0.0",
    "@types/babel__core": "^7.1.19",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-infinite-scroller": "^1.2.2",
    "@types/react-lazyload": "^3.2.0",
    "@types/redux-persist": "^4.3.1",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vitejs/plugin-react": "^1.0.7",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.2.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "npm": "^8.5.2",
    "prettier": "^2.5.1",
    "react-error-overlay": "6.0.9",
    "rollup-plugin-polyfill-node": "^0.8.0",
    "typechain": "^7.0.1",
    "typescript": "^4.6.3",
    "vite": "^2.8.6"
  }
}
